{
    "name": "DowebDomobile/cakephp-users-plugin",
    "description": "Users plugin for CakePHP3",
    "type": "cakephp-plugin",
    "authors": [
        {
            "name": "Ivan Vorsin",
            "email": "ivanboroda@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.16",
        "cakephp/plugin-installer": "*",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Dwdm\\Users\\": "src"

        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dwdm\\Users\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
