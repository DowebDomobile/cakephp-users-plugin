{
    "name": "DowebDomobile/cakephp-users-plugin",
    "description": "Users plugin for CakePHP3",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "User\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "User\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
